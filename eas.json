{
  "cli": {
    "version": ">= 16.19.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "image": "latest",
        "buildConfiguration": "Release"
      },
      "env": {
        "NODE_OPTIONS": "--max-old-space-size=3072",
        "EXPO_NO_METRO_CACHE": "1"
      },
      "cache": {
        "key": "production-cache-v2",
        "paths": [
          "node_modules",
          "ios/Pods"
        ]
      }
    }
  },
  "submit": {
    "production": {}
  }
}
