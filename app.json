{
  "expo": {
    "name": "Quack Plan v1",
    "slug": "quack-plan",
    "entryPoint": "./app/index.js",
    "icon": "./assets/images/quackplanicon.png",
    "notification": {
      "icon": "./assets/images/notification-icon.png", 
      "color": "#ef4444" 
    },
    "ios": {
      "bundleIdentifier": "com.anonymous.quackplan"
    },
    "android": {
      "package": "com.anonymous.quackplan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/quackplanicon.png",
        "backgroundColor": "#ffffff"
      },
      "notification": {
        "icon": "./assets/images/notification-icon.png",
        "color": "#ef4444"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#ef4444"
        }
      ]
    ]
  }
}
